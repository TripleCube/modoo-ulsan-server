{
  "name": "modoo-ulsan-server",
  "version": "0.2.0-alpha",
  "description": "Backend for Modoo Ulsan",
  "repository": "https://github.com/TripleCube/modoo-ulsan-server",
  "author": "TripleCube <can@live.in>",
  "license": "MIT",
  "scripts": {
    "start": "yarn build && node dist/server.js",
    "build": "yarn clean && babel src -d dist -s inline",
    "clean": "rimraf dist",
    "serve": "nodemon src/server.js",
    "add:migration": "sequelize migration:generate --name $NAME",
    "migrate": "sequelize db:migrate",
    "migrate:reset": "sequelize db:migrate:undo:all"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mariadb": "^2.4.0",
    "sequelize": "^6.2.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/register": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5",
    "rimraf": "^3.0.2",
    "sequelize-cli": "^6.1.0"
  }
}
